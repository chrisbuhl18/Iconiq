{
  "git": {
    "deploymentEnabled": {
      "main": false,
      "preview": true
    }
  },
  "github": {
    "enabled": true,
    "silent": false
  },
  "ignoreCommand": "if [[ \"$VERCEL_GIT_COMMIT_REF\" == \"main\" ]]; then exit 0; else exit 1; fi",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "golumio.vercel.app"
        }
      ],
      "destination": "https://golumio.vercel.app/$1",
      "permanent": false
    }
  ]
}
